// Test 1: Basic insertion and retrieval
var apple = "red";
var banana = "yellow";
var cherry = "red";
print apple;    // red
print banana;   // yellow
print cherry;   // red

// Test 2: Update existing keys
apple = "green";
print apple;    // green

// Test 3: Multiple variables to test capacity growth and collision handling
var v1 = 1;
var v2 = 2;
var v3 = 3;
var v4 = 4;
var v5 = 5;
var v6 = 6;
var v7 = 7;
var v8 = 8;
var v9 = 9;
var v10 = 10;

// Test 4: Verify all values are correct
