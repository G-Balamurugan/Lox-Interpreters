// Test 5: Nested scopes (tests tableAddAll when creating new environments)
{
    var scoped = "inner";
    var v1 = 100;  // shadows outer v1
    print scoped;  // inner
    print v1;      // 100
}
print v1;  // 1 (outer scope restored)

